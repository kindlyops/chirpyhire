= title "Messages | ChirpyHire"
= content_for :container_class, 'container-fluid'
= content_for :body_class, 'chat'

= render partial: "messages/header"
.client_body
  .client_messages
    .footer
      form.message
        textarea.message-input.focus data-autoresize=true rows="1" autocomplete="off" autocorrect="off" spellcheck="true" placeholder="Message wayne"
    #messages_container role="main" 
      #msgs_scroller_div.message_pane_scroller style=("height: 600px;") tabindex="-1" 
        #end_div.relative.mini.help style=("height: 190px;") 
